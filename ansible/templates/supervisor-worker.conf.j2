[program:{{app_name}}@{{username}}]
user            =   {{username}}
directory       =   {{app_root}}
command         =   {{app_root}}/run.sh
process_name    =   %(process_num)02d

numprocs        =   1
autostart       =   true
autorestart     =   true

redirect_stderr =   true
stdout_logfile  =   var/log/%(program_name)s.log
